{
  "name": "esbuild-plugin-license",
  "type": "module",
  "version": "1.2.3",
  "description": "esbuild plugin for generating license for your bundle",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js"
  },
  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "rimraf dist && tsup --watch --sourcemap"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "license",
    "esbuild"
  ],
  "author": "Yiming Li",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/upupming/esbuild-plugin-license/issues"
  },
  "homepage": "https://github.com/upupming/esbuild-plugin-license#readme",
  "devDependencies": {
    "@types/node": "22.5.5",
    "esbuild": "0.23.1",
    "radashi": "12.2.0-beta.af825f4",
    "read-pkg-up": "9.0.0",
    "rimraf": "^6.0.1",
    "tsup": "8.3.0",
    "typescript": "5.6.2"
  },
  "peerDependencies": {
    "esbuild": "*"
  }
}
